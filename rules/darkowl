! model 	=	keycodes	geometry
  n52		=	belkin(n52)	belkin(n52)

! model		layout		=	symbols
  n52		us	=	belkin(n52)

! model		layout	=	compat		types
  *		*	=	complete	complete

! option		=	symbols
  lv3:switch		=	+level3(switch)